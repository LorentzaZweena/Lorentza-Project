<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="SehatQ.ProfileActivity">

    <ImageButton
        android:id="@+id/ibBackSehatQ"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_back"
        android:layout_marginTop="30sp"
        android:layout_marginStart="30sp"
        />

    <TextView
        android:id="@+id/teks59"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="Profile dokter"
        android:layout_marginTop="28sp"
        android:textSize="20sp"
        android:textStyle="bold"
        />

    <ImageButton
        android:id="@+id/ibShareShehatQ"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/teks59"
        android:background="@drawable/baseline_share_24"
        android:layout_marginTop="30sp"
        android:layout_marginStart="60dp"
        />

    <ImageView
        android:id="@+id/ivDokter"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/image87"
        android:layout_below="@id/teks59"
        android:layout_marginTop="20sp"
        />

    <ImageView
        android:id="@+id/gambar14"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:src="@drawable/image57"
        android:layout_below="@id/ivDokter"
        android:layout_marginStart="33sp"
        android:layout_marginTop="20sp"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/gambar14"
        android:layout_below="@id/ivDokter"
        android:text="Kesehatan lansia"
        android:textSize="16dp"
        android:layout_marginTop="21sp"
        android:layout_marginStart="10dp"
        android:textStyle="bold"
        android:textColor="#5EB1B3"
        />
</RelativeLayout>