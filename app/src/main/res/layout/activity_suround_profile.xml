        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/garis2"
        android:layout_toRightOf="@+id/teks126"
        android:text="784"
        android:textSize="20sp"
        android:layout_marginStart="70sp"
        android:layout_marginTop="20sp"
        android:textStyle="bold"
        />

    <TextView
        android:id="@+id/teks128"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/teks125"
        android:text="Favorites"
        android:layout_marginStart="70sp"
        android:layout_toRightOf="@id/teks126"
        android:layout_marginTop="5dp"
        android:textColor="#858795"
        android:textStyle="bold"
        android:textSize="14dp"
        />

    <Button
        android:id="@+id/garis3"
        android:layout_width="match_parent"
        android:layout_below="@id/teks128"
        android:layout_height="5dp"
        android:layout_marginTop="25dp"
        android:backgroundTint="#E1E1E4"
        />

    <TextView
        android:id="@+id/teks129"
