        android:layout_marginStart="22sp"
        android:textSize="16sp"
        android:layout_marginTop="15sp"
        android:textColor="#888282"
        android:textStyle="bold"
        android:drawableStart="@drawable/baseline_location_on_24"
        android:drawablePadding="5dp"
        />

    <androidx.cardview.widget.CardView
        android:layout_width="50dp"
        android:id="@+id/cvEdit"
        android:layout_below="@id/gambar47"
        android:layout_toRightOf="@id/teks120"
        android:layout_marginTop="55sp"
        app:cardCornerRadius="30sp"
        android:layout_marginStart="90sp"
        android:layout_height="50dp">

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/baseline_edit_24"
            android:background="@drawable/stroke"
            android:padding="60dp"
            android:backgroundTint="@color/white"
            />
    </androidx.cardview.widget.CardView>
    <Button
        android:id="@+id/garis"
        android:layout_width="wrap_content"
        android:layout_below="@id/teks121"
        android:layout_height="1dp"
        android:layout_marginStart="28sp"
        android:layout_marginTop="25dp"
        android:backgroundTint="#B5B5BA"
        />

    <TextView
        android:id="@+id/teks122"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/garis"
        android:text="In the symphony of life, let your heart play the most captivating melody, for the dance of joy follows the rhythm of passion."
