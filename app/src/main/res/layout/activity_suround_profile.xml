<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".suround.SuroundProfileActivity">

    <ImageView
        android:id="@+id/gambar47"
        android:layout_width="match_parent"
        android:layout_height="190dp"
        android:background="@drawable/image148"
        android:scaleType="centerCrop"
        />

    <ImageButton
        android:id="@+id/ibBackSuroundProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/stroke"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/ic_back"
        android:padding="10dp"
        android:backgroundTint="@color/white"
        />

    <ImageButton
        android:id="@+id/ibShareProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/baseline_share_24"
        android:layout_toRightOf="@id/ibBackSuroundProfile"
        android:layout_marginTop="36dp"
        android:layout_marginStart="260dp"
        />

    <androidx.cardview.widget.CardView
        android:layout_width="100dp"
        android:id="@+id/cvProfile"
        android:layout_below="@id/ibBackSuroundProfile"
        app:cardCornerRadius="100dp"
        android:layout_marginTop="50sp"
        android:layout_marginStart="25dp"
        android:layout_height="100dp">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/image137"
            android:scaleType="centerCrop"
            />
    </androidx.cardview.widget.CardView>
    <TextView
        android:id="@+id/teks120"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Transmogrifier Biopsy"
        android:textSize="20sp"
        android:layout_below="@id/cvProfile"
        android:textStyle="bold"
        android:layout_marginStart="25sp"
        android:layout_marginTop="20sp"
        />

    <TextView
        android:id="@+id/teks121"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/teks120"
